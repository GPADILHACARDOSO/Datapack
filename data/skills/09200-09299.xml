    <skill id="9206" name="Fast Moving" max-level="2" action="A2">
        <!-- 
			level 1: For 30 sec., Speed +95.
			level 2: For 35 sec., Speed +95.
		-->
        <icon initial="icon.skill9206"/>
        <attributes magic-critical-rate="5.0" without-action="true">
            <magic-level initial="1"/>
            <cast-range initial="-1"/>
            <reuse initial="45000"/>
            <effect-point initial="1"/>
        </attributes>
        <target/>
        <abnormal type="SPECIAL_RIDE">
            <level initial="1"/>
            <time initial="30"/>
        </abnormal>
        <effects>
            <speed>
                <power initial="95.0"/>
            </speed>
        </effects>
    </skill>
