<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<list xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://l2j.org" xsi:schemaLocation="http://l2j.org skills.xsd">
    <skill id="62001" name="White Assassin Transformation" action="A2">
        <!-- You are transformed into White Assassin. For 20 min., Max HP/ MP +7%%, P./ M. Accuracy +2, P./ M. Atk. +3%%, P./ M. Def. +3%%, Atk. Spd. +5%%, M. Skill Critical Rate +20, Speed +5. The effect remains after death. -->
        <icon initial="icon.scroll_of_verification_i06"/>
        <attributes stay-after-death="true">
            <cast-range initial="-1"/>
            <reuse initial="60000"/>
            <effect-point initial="1"/>
        </attributes>
        <target/>
        <abnormal type="CHANGEBODY" visual="WHITE_ASSASSIN_SUIT">
            <level initial="1"/>
            <time initial="1200"/>
        </abnormal>
        <effects>
            <vital-stat-modify mode="PER" stat="MAX_HP">
                <power initial="7.0"/>
            </vital-stat-modify>
            <vital-stat-modify mode="PER" stat="MAX_MP">
                <power initial="7.0"/>
            </vital-stat-modify>
            <speed>
                <power initial="5.0"/>
            </speed>
            <stat-modify mode="PER" stat="PHYSICAL_ATTACK_SPEED" stat-add="PHYSICAL_ATTACK_SPEED">
                <power initial="5.0"/>
            </stat-modify>
            <stat-modify stat="ACCURACY">
                <power initial="2.0"/>
            </stat-modify>
            <stat-modify stat="ACCURACY_MAGIC">
                <power initial="2.0"/>
            </stat-modify>
            <stat-modify stat="MAGIC_CRITICAL_RATE">
                <power initial="20.0"/>
            </stat-modify>
            <stat-modify mode="PER" stat="PHYSICAL_ATTACK">
                <power initial="3.0"/>
            </stat-modify>
            <stat-modify mode="PER" stat="MAGIC_ATTACK">
                <power initial="3.0"/>
            </stat-modify>
            <stat-modify mode="PER" stat="PHYSICAL_DEFENCE">
                <power initial="3.0"/>
            </stat-modify>
            <stat-modify mode="PER" stat="MAGICAL_DEFENCE">
                <power initial="3.0"/>
            </stat-modify>
        </effects>
    </skill>
    <skill id="62002" name="Mount Griffin" action="A2">
        <!-- Allows to mount Griffin. -->
        <icon initial="icon.griffin_vehicle"/>
        <attributes blocked-on-olympiad="true" irreplacable="true" magic-critical-rate="5.0" stay-after-death="true">
            <cast-range initial="-1"/>
            <effect-point initial="1"/>
            <hit-time initial="1000"/>
        </attributes>
        <target/>
        <abnormal type="TRANSFORM">
            <level initial="1"/>
            <time initial="3600"/>
        </abnormal>
        <conditions>
            <condition name="CanTransform"/>
        </conditions>
        <effects>
            <transformation id="204"/>
        </effects>
    </skill>
</list>
