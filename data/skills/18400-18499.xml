<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<list xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://l2j.org" xsi:schemaLocation="http://l2j.org skills.xsd">
    <skill id="18448" name="Scroll: Summon Piero" action="A1">
        <!-- Summon NPC Piero -->
        <icon initial="icon.skill0000"/>
        <attributes>
            <cast-range initial="-1"/>
            <reuse initial="10000"/>
            <hit-time initial="3000"/>
        </attributes>
        <target/>
        <effects>
            <summon-npc count="1" despawn-delay="180000">
                <npc initial="34084.0"/>
            </summon-npc>
        </effects>
    </skill>
    <skill id="18449" name="Magic Number Cube" action="A1">
        <!-- Group gift distribution -->
        <icon initial="icon.skill0000"/>
        <attributes>
            <cast-range initial="1500"/>
            <reuse initial="6000"/>
            <hit-time initial="1500"/>
        </attributes>
        <target/>
    </skill>
    <skill id="18450" name="Confirm Number" max-level="6" action="A1" type="MAGIC">
        <!-- 
			level 1: Shows number 1.
			level 2: Shows number 2.
			level 3: Shows number 3.
			level 4: Shows number 4.
			level 5: Shows number 5.
			level 6: Shows number 6.
		-->
        <icon initial="icon.skill0000"/>
        <attributes>
            <cast-range initial="900"/>
            <effect-point initial="204"/>
            <hit-time initial="3000"/>
        </attributes>
        <consume>
            <mana initial="75"/>
        </consume>
        <target/>
    </skill>
    <skill id="18451" name="Anniversary Effect" max-level="2" action="A2">
        <!-- 
			level 1: Acquired XP/ SP +11%. A chance to obtain a 11th anniversary coin when the buff is cancelled.  Note! The coins can't be obtained if your character is dead or has no free space in the inventory.
			level 2: When the buff effect is over you can get 11 Anniversary Coin.
		-->
        <icon initial="icon.ev_buff_11th"/>
        <attributes>
            <cast-range initial="900">
                <value level="2">-1</value>
            </cast-range>
            <effect-point initial="1"/>
        </attributes>
        <target/>
        <abnormal>
            <time initial="3600">
                <value level="2">7200</value>
            </time>
        </abnormal>
        <effects>
            <stat-modify mode="PER" stat="BONUS_EXP" stat-add="BONUS_EXP">
                <power initial="11.0">
                    <value level="2">30.0</value>
                </power>
            </stat-modify>
        </effects>
    </skill>
    <skill id="18472" name="Giveaway Number 1" max-level="6" action="A1">
        <!-- none -->
        <icon initial="icon.skill0000"/>
        <attributes>
            <cast-range initial="-1"/>
        </attributes>
        <target/>
    </skill>
    <skill id="18475" name="Anniversary Coin" action="A2">
        <!--  -->
        <icon initial="icon.ev_buff_11th"/>
        <attributes>
            <cast-range initial="-1"/>
            <effect-point initial="1"/>
        </attributes>
        <target/>
    </skill>
</list>
