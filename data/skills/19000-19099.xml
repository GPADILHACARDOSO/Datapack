    <skill id="19009" name="Clan Advent" max-level="10" action="A2">
        <!-- 
			level 1: When a clan leader is online, the following effects are applied for clan members.  P. Atk. +5 M. Atk. +5
			level 2: When a clan leader is online, the following effects are applied for clan members.  P. Atk. +10 M. Atk. +10
			level 3: When a clan leader is online, the following effects are applied for clan members.  P. Atk. +15 M. Atk. +15
			level 4: When a clan leader is online, the following effects are applied for clan members.  P. Atk. +25 M. Atk. +25
			level 5: When a clan leader is online, the following effects are applied for clan members.  P. Atk. +50 M. Atk. +50
			level 6: When a clan leader is online, the following effects are applied for clan members.  P. Atk. +100 M. Atk. +100
			level 7: When a clan leader is online, the following effects are applied for clan members.  P. Atk. +200 M. Atk. +200
			level 8: When a clan leader is online, the following effects are applied for clan members.  P. Atk. +200 M. Atk. +200 P. Def. +50 M. Def. +50
			level 9: When a clan leader is online, the following effects are applied for clan members.  P. Atk. +200 M. Atk. +200 P. Def. +100 M. Def. +100
			level 10: When a clan leader is online, the following effects are applied for clan members.  P. Atk. +200 M. Atk. +200 P. Def. +200 M. Def. +200
		-->
        <icon initial="icon.skill19009_1">
            <value level="2">icon.skill19009_2</value>
            <value level="3">icon.skill19009_3</value>
            <value level="4">icon.skill19009_4</value>
            <value level="5">icon.skill19009_5</value>
            <value level="6">icon.skill19009_6</value>
            <value level="7">icon.skill19009_7</value>
            <value level="8">icon.skill19009_8</value>
            <value level="9">icon.skill19009_9</value>
            <value level="10">icon.skill19009_10</value>
        </icon>
        <attributes dispellable="false" irreplacable="true" magic-critical-rate="5.0" stay-after-death="true">
            <magic-level initial="-2"/>
            <cast-range initial="-1"/>
            <effect-point initial="100"/>
        </attributes>
        <target affect-min="50" affect-random="50" object="FRIEND" range="1000" scope="PLEDGE"/>
        <abnormal type="CLAN_ADVENT">
            <level initial="1"/>
            <time initial="-1"/>
        </abnormal>
        <effects>
            <stat-modify mode="PER" stat="PHYSICAL_ATTACK">
                <power initial="2.0"/>
            </stat-modify>
            <stat-modify mode="PER" stat="MAGIC_ATTACK">
                <power initial="2.0"/>
            </stat-modify>
        </effects>
    </skill>
