    <skill id="13661" name="Brooch: Additional Inventory Slots" max-level="5">
        <!-- 
			level 1: <Effect> Inventory Slots +5.
			level 2: <Effect> Inventory Slots +9.
			level 3: <Effect> Inventory Slots +12.
			level 5: <Effect> Inventory Slots +15.
		-->
