<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<list xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://l2j.org" xsi:schemaLocation="http://l2j.org skills.xsd">
    <skill id="8256" name="Summon Agathion Joy" action="A1">
        <!-- Summons an agathion. -->
        <icon initial="icon.etc_summon_aga_agit_i00"/>
        <attributes magic-critical-rate="5.0">
            <magic-level initial="85"/>
            <cast-range initial="-1"/>
            <hit-time initial="3000"/>
        </attributes>
        <target/>
        <effects>
            <summon-agathion id="16056"/>
        </effects>
    </skill>
    <skill id="8292" name="Olympiad Hero's Ring">
        <!-- Hold Resistance and Atk. Rate +30%%, P. Critical Damage +15%%, P. Accuracy +2. -->
        <icon initial="icon.accessory_tateossian_ring_i00"/>
        <attributes magic-critical-rate="5.0"/>
        <target/>
        <effects>
            <attack-trait>
                <trait type="HOLD">
                    <power initial="30.000001907348633"/>
                </trait>
            </attack-trait>
            <defence-trait>
                <trait type="HOLD">
                    <power initial="30.000001907348633"/>
                </trait>
            </defence-trait>
            <stat-modify stat="ACCURACY" stat-add="ACCURACY">
                <power initial="2.0"/>
            </stat-modify>
            <stat-modify mode="PER" stat="CRITICAL_DAMAGE" stat-add="CRITICAL_DAMAGE_ADD">
                <power initial="15.0"/>
            </stat-modify>
        </effects>
    </skill>
    <skill id="8293" name="Olympiad Hero's Earring">
        <!-- Silence/ Fear/ Stun Resistance and Atk. Rate +20%%. With a certain chance, absorbs 4%% of inflicted damage as HP. -->
        <icon initial="icon.accessory_tateossian_earring_i00"/>
        <attributes magic-critical-rate="5.0"/>
        <target/>
        <effects>
            <attack-trait>
                <trait type="SHOCK">
                    <power initial="20"/>
                </trait>
                <trait type="DERANGEMENT">
                    <power initial="20"/>
                </trait>
            </attack-trait>
            <defence-trait>
                <trait type="SHOCK">
                    <power initial="20"/>
                </trait>
                <trait type="DERANGEMENT">
                    <power initial="20"/>
                </trait>
            </defence-trait>
            <vampiric-attack chance="80">
                <power initial="4.0"/>
            </vampiric-attack>
        </effects>
    </skill>
    <skill id="8294" name="Olympiad Hero's Necklace">
        <!-- Paralysis/ Stun/ Sleep Resistance +15%%, Paralyzing/ Stunning attack bonus +15%%. Skill Cooldown -5%%. Returns 4%% of received damage to the target. -->
        <icon initial="icon.accessory_tateossian_necklace_i00"/>
        <attributes magic-critical-rate="5.0"/>
        <target/>
        <effects>
            <attack-trait>
                <trait type="SHOCK">
                    <power initial="15"/>
                </trait>
                <trait type="SLEEP">
                    <power initial="15"/>
                </trait>
                <trait type="PARALYZE">
                    <power initial="15"/>
                </trait>
            </attack-trait>
            <defence-trait>
                <trait type="SHOCK">
                    <power initial="0.15000000596046448"/>
                </trait>
                <trait type="SLEEP">
                    <power initial="0.15000000596046448"/>
                </trait>
                <trait type="PARALYZE">
                    <power initial="0.15000000596046448"/>
                </trait>
            </defence-trait>
            <stat-modify stat="REFLECT_DAMAGE_PERCENT">
                <power initial="4.0"/>
            </stat-modify>
            <reuse skill-type="PHYSIC">
                <power initial="-5.0"/>
            </reuse>
        </effects>
    </skill>
</list>
