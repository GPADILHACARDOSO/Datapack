<?xml version="1.0" encoding="UTF-8"?>
<list xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://l2j.org" xsi:schemaLocation="http://l2j.org items.xsd">
	<item id="13009" name="Inventory3" type="NONE">
		<restriction tradable="false" dropable="false" sellable="false" private-sellable="false" depositable="false" clan-depositable="false" castle-depositable="false" freightable="false"/>
		<attributes immediate-effect="true" ex-immediate-effect="true"/>
	</item>

	<item id="13015" name="My Teleport Book (Exchangeable)" type="NONE">
	<!-- Increases My Teleport locations by 3 slots, which can be used to save specific locations. My Teleport locations can be expanded up to 48 slots. -->
		<restriction sellable="false" clan-depositable="false" castle-depositable="false" freightable="false"/>
		<attributes weight="5" action="CALL_SKILL"/>
		<skill-reducer type="ItemSkills">
			<skill id="2587" value="-1"/>  <!-- Additional My Teleport slots -->
		</skill-reducer>
	</item>

	<item id="13016" name="My Teleport Scroll" type="TELEPORT_BOOKMARK">
	<!-- This consumable item allows you to magically transport yourself to a specific location. -->
		<restriction sellable="false" clan-depositable="false" castle-depositable="false" freightable="false"/>
		<attributes immediate-effect="true"/>
	</item>

	<item id="13021" name="Color Title" type="NICK_COLOR_OLD">
	<!-- Allows to change the character's title color.\nUse the '/ resetname' command to return to the default color. The title disappears after joining the clan of Lv. 3 or lower.\nCharacters of Lv. 90+ can choose the <font color="00DCFF">sky-blue</font> color. -->
		<restriction sellable="false" clan-depositable="false" castle-depositable="false" freightable="false"/>
		<attributes weight="5" immediate-effect="true" action="NICK_COLOR"/>
		<action handler="NicknameColor"/>
	</item>

	<armor id="13025" name="Agathion Joy - Not Available" type="NONE" body-part="TALISMAN">
	<!-- The item is not available. Available for 7 days. Enables to summon Agathion Joy when equipped.\n\n<Active Skills>\n1. Agathion Teleport: teleports to Giran, Gludio, Dion, Oren, Hunters Village (cooldown: 1 min.).\n\n<Item Skill>\n1. Evidence Chase: Speed +66 for 15 sec. \n\n<Item Skill>2. Secret Investigation: monsters don't attack first. -->
		<restriction freightable="false" tradable="false" dropable="false" sellable="false" private-sellable="false"/>
		<attributes weight="30" duration="43200" enchant-enabled="false" />
		<crystal />
		<skills>
			<skill id="8256" value="-1"/>  <!-- Summon Agathion Joy -->
			<skill id="5955" value="-1"/>  <!-- Agathion Cute Trick - Joy -->
			<skill id="3267" type="ON_UNEQUIP" value="-1"/>  <!-- Seal Agathion -->
			<skill id="39056" value="-1"/>  <!-- Agathion Teleport - Oren -->
			<skill id="39055" value="-1"/>  <!-- Agathion Teleport: Giran -->
			<skill id="39054" value="-1"/>  <!-- Agathion Teleport: Dion -->
			<skill id="39053" value="-1"/>  <!-- Agathion Teleport: Gludio -->
			<skill id="39100" value="-1"/>  <!-- Agathion Teleport - Hunters' Village -->
			<skill id="39101" value="-1"/>  <!-- Evidence Chase -->
			<skill id="39102" value="-1"/>  <!-- Secret Investigation -->
		</skills>
	</armor>

</list>