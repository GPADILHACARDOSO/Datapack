<?xml version="1.0" encoding="UTF-8"?>
<list xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://l2j.org" xsi:schemaLocation="http://l2j.org items.xsd">
	<item id="48400" name="Items Seal Scroll" type="SEAL_SCROLL">
	<!-- A scroll for sealing items.\nThe sealed item cannot be augmented, exchanged, dropped, destroyed, or stored in the clan warehouse.\nYou can seal weapons, armor, some accessories. Temporary items (and some others) cannot be sealed.\nUse Item Unseal Scroll to remove the seal. -->
		<restriction tradable="false" dropable="false" private-sellable="false" clan-depositable="false" castle-depositable="false" freightable="false"/>
		<attributes price="1000"/>
		<random_craft exp="500" commission="10"/>
	</item>

	<item id="48401" name="Item Unseal Scroll" type="UNSEAL_SCROLL">
	<!-- Use to unseal a sealed item. -->
		<restriction stackable="true" tradable="false" dropable="false" sellable="false" private-sellable="false" depositable="false" clan-depositable="false" castle-depositable="false" freightable="false"/>
		<attributes/>
	</item>

	<item id="48411" name="Item Unseal Scroll (15-min) Box" type="NONE">
	<!-- Double-click to obtain 10 Item Unseal Scrolls (15-min). -->
		<restriction stackable="true" tradable="false" dropable="false" sellable="false" private-sellable="false" depositable="false" clan-depositable="false" castle-depositable="false" freightable="false"/>
		<attributes weight="10" immediate-effect="true" action="PEEL"/>
	</item>

</list>