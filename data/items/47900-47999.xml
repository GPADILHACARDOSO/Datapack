<list xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://l2j.org" xsi:schemaLocation="http://l2j.org items.xsd">
	<weapon id="47927" name="Magic Fan - Blue" type="ROD" body-part="RIGHT_HAND" magic="true">
	<!-- A magic fan that can be used for making fire and cooling in hot weather. A beautiful accessory for every costume.  -->
		<restriction tradable="false" dropable="false" sellable="false" private-sellable="false"/>
		<attributes weight="150" commission-type="OTHER_ITEM" enchant-enabled="false"/>
		<crystal />
		<damage radius="40" angle="240"/>
		<stats>
			<stat type="ACCURACY" value="8.0"/>
			<stat type="CRITICAL_RATE" value="6.0"/>
			<stat type="MAGIC_ATTACK" value="1.0"/>
			<stat type="PHYSICAL_ATTACK" value="1.0"/>
			<stat type="PHYSICAL_ATTACK_RANGE" value="40.0"/>
			<stat type="PHYSICAL_ATTACK_SPEED" value="379.0"/>
			<stat type="RANDOM_DAMAGE" value="1.0"/>
		</stats>
	</weapon>

	<item id="47928" name="Magic Woodpile" type="NONE">
	<!-- Double-click to use a magic woodpile for one player. If you have the Magic Fan equipped, you can use the skills Breeze and Gust to set fire to the woodpile. \n -->
		<restriction/>
		<attributes action="SKILL_REDUCE"/>
	</item>

	<item id="47929" name="Large Magic Woodpile" type="NONE">
	<!-- Double-click to use a magic woodpile for party use. If you have the Magic Fan equipped, you can use the skills Breeze and Gust to set fire to the woodpile. \n -->
		<restriction/>
		<attributes action="SKILL_REDUCE"/>
	</item>

