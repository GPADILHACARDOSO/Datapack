	<item id="48046" name="Scroll: Enchant Agathion" type="ENCHANT_AGATHION">
	<!-- Only for Zodiac agathions. Increases M. Def. The enchanting is safe up to +3, the maximum enchant value is +10. Starting from +1, the agathion's secondary skills are enhanced with each enchant value. -->
		<restriction/>
		<attributes action="SKILL_REDUCE"/>
		<action handler="EnchantScrolls"/>
	</item>

	<item id="48047" name="Scroll: Enchant High-grade Agathion" type="MULTI_ENCHANT_AGATHION">
